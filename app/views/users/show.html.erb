<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <h3><%= @user.name %> data</h3>
    <dl class="dl-horizontal">
      <dt>Name</dt>
      <dd><%= @user.name %></dd>
      <dt>Email</dt>
      <dd><%= @user.email %></dd>
      <dt>Phone</dt>
      <dd><%= @user.phone %></dd>
      <dt>Admin?</dt>
      <dd><%= @user.admin %></dd>
      <dt>Created at</dt>
      <dd><%= @user.created_at %></dd>
      <dt>Confirmation at</dt>
      <dd><%= @user.confirmed_at %></dd>
    </dl>
    <%= link_to "Go back", :back %>
  </div>
</div>

html, body{
  height: 95.3%;
}

.browse-item{
  width: 50%;
  min-width: 300px;
  height: 300px;
  padding: 0;
  font-family: "Calibri";
  padding: 5px;
}

.browse-item-data{
  height: 60px;
  width: 100%;
  border: 1px solid rgb(248,248,248);
  background-color: rgb(248,248,248);
}

.browse-item-name{
  color: black;
  margin: 15px;
  display: block;
  font-size: 20px;
}

.browse-item-name-frame{
  height: 100%;
  width: 70%;
  float: left;
}
.browse-item-picture-frame{
  height: 230px; /* Each item block has 300px, minus 10px padding.
   We leave 60px for data and 230px for picture*/

}

.browse-item-social{
  position: absolute;
  right: 5px;
  top: 5px;
  width: 42px;
}

.browse-item-picture{
  height: 100%;
  display: block;
  margin-right: auto;
  margin-left: auto;
}

.browse-item-price{
  color: black;
  margin: 15px;
  display: block;
  font-size: 20px;
  font-weight: bold;
  text-align: right;
}

.browse-item-price-frame{
  height: 100%;
  margin-left: 70%;
}

.email{
  background-color:#CCCCCC;
  height: 2px;
}
.item-description{
  max-width: 100%;
}
.item-header{
}

.item-highlight{
}

.item-details{
  padding: 10px;
}

.item-data-frame{
}

.item-description{
  color: black;
  margin: 15px 0px 15px 0px;
  font-size: 14px;
}

.item-name{
  float: left;
  font-size: 30px;
  font-weight: bold;
  padding-left: 10px;
  width:60%;
}

.item-picture{
  height: 80%;
  display: block;
  margin-right: auto;
  margin-left: auto;
}

.item-picture-frame{
  height: 400px;
  padding: 0;
}

.item-thumblist{
  height: 20%;
  padding-top: 10px;
  padding-bottom: 10px;
}

.item-price{
  margin: 15px 0px 15px 0px;
  font-size: 30px;
  font-weight: bold;
}

.item-social{
  padding-top: 20px;
  padding-right: 10px;
  float:right;
  width:40%;
}

.item-thumbnail{
  height:100%;
  margin-left: 5px;
  margin-right: 5px;
}

.jumbotron-main{
  height: 500px;
  margin-top: -10px;
  padding-top: 150px;
  background-position: 50% 90%;
  background-size: cover;
  width: 100%;
}

.jumbotron-text{
  color: rgb(240,240,240);
  text-align: center;
}

.summary-item-frame{
}

.summary-item-name{
  color: black;
  margin: 15px;
  display: block;
  font-size: 18px;
}

.summary-item-picture-frame{
  height: 100%;
}

.summary-item-picture{
  max-height: 150px;
  display: block;
  margin-right: auto;
  margin-left: auto;
}

.summary-item-data{
  height: 150px;
  background-color: rgb(248,248,248);
}

.main{
  min-height: 100%;
  padding-top: 30px;
  padding-bottom: 30px;
}

.required:after{
  content: " *";
  color: #e32;
}
